# 데이터베이스 연결 정보
DATABASE_URL=postgresql://username:password@localhost:5432/precedent_db

# 법제처 OPEN API 인증 정보
LAW_API_EMAIL_ID=your_email_id

# 임베딩 모델 설정
# SentenceTransformer 모델 사용 중: 'jhgan/ko-sroberta-multitask'
# 별도의 환경 변수는 필요하지 않지만, OpenAI 모델을 대체로 사용하는 경우 필요할 수 있음
# OPENAI_API_KEY=your_openai_api_key

# API 요청 제한 설정
# API 호출 간격(초) - 기본값 1초
API_CALL_DELAY=1

# 최대 API 요청 수 - 기본값 100
MAX_API_CALLS=100

# 서버 설정 (FastAPI 서버 실행 시 사용)
HOST=0.0.0.0
PORT=8000
DEBUG=False

# 로깅 설정
LOG_LEVEL=INFO